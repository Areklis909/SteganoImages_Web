{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "areklis909/stegano-images-web:0.0.1",
        "hostname": "frontend",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["backend"],
        "memory": 100
      },
      {
        "name": "backend",
        "image": "areklis909/stegano-images-server:0.0.2",
        "hostname": "backend",
        "essential": false,
        "memory": 100
      }
    ]
}